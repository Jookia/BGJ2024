  },
  "homepage": "https://github.com/Jookia/BGJ2024#readme",
  "devDependencies": {
    "autoprefixer": "^10.4.17",
    "clean-html": "^2.0.1",
    "postcss": "^8.4.35",
    "prettier": "^3.2.4",
    "tailwindcss": "^3.4.1"
  },
  "dependencies": {
    "clsx": "^2.1.0",
    "next": "^14.1.0",
    "react": "^18.2.0",
