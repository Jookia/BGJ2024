{
  "name": "bgj2024",
  "version": "1.0.0",
  "description": "Blind Game Jam 2024 game",
  "private": true,
  "scripts": {
    "check": "npx prettier -w src && npx clean-html public/index.html --in-place",
    "dev": "npx tailwindcss -i ./src/app/globals.css -o ./public/style.css && npx esbuild src/app/index.js --loader:.js=jsx --sourcemap=inline --bundle --outdir=public/ --servedir=public/ --serve=127.0.0.1:4000",
    "clean": "rm public/index.js public/index.css"
  },
  "keywords": [
    "blind",
    "accessible",
    "accessibility",
    "game jam",
    "video game",
    "game"
  ],
  "author": "Jookia",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jookia/BGJ2024.git"
  },
  "bugs": {
    "url": "https://github.com/Jookia/BGJ2024/issues"
  },
  "homepage": "https://github.com/Jookia/BGJ2024#readme",
  "devDependencies": {
    "autoprefixer": "^10.4.17",
    "clean-html": "^2.0.1",
    "esbuild": "^0.20.0",
    "postcss": "^8.4.35",
    "prettier": "^3.2.4",
    "tailwindcss": "^3.4.1"
  },
  "dependencies": {
    "clsx": "^2.1.0"
  }
}
